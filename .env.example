# Environment Variables for AI Logo Studio
# Copy this file to .env and fill in your actual API keys
# NEVER commit .env files with real API keys to git!

# OpenAI API Key for DALL-E 3 generation
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-openai-api-key-here

# Stability AI API Key for Stable Diffusion
# Get your key from: https://platform.stability.ai/account/keys
STABILITY_API_KEY=your-stability-api-key-here

# Database Configuration
DATABASE_URL=postgresql+asyncpg://app:app@db:5432/appdb

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:9092

# S3/LocalStack Configuration
S3_ENDPOINT_URL=http://localstack:4566
AWS_REGION=eu-central-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
S3_BUCKET=ai-logo-studio

# AI Engine Configuration
USE_NEW_AI=true

# Development Settings
DEBUG=true
LOG_LEVEL=INFO